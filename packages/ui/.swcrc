{
  "minify": true,
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": true,
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "react": {
        "runtime": "automatic",
        "development": true,
        "useBuiltins": false
      }
    },
    "minify": {
      "compress": {
        "unused": true
      },
      "mangle": true
    }
  }
}
